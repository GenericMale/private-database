{
  "name": "private-database",
  "version": "0.0.1",
  "description": "Private Database",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GazonkFoo/private-database.git"
  },
  "license": "MIT",
  "main": "dist/server/main.js",
  "scripts": {
    "start": "node dist/server/main.js",
    "build": "run-s clean build:* copy",
    "build:client": "ng build --prod",
    "build:server": "tsc --project src/server",
    "clean": "rimraf dist",
    "copy": "copyfiles -u 2 \"src/server/templates/**/*\" dist/server/",
    "serve": "run-p watch nodemon",
    "watch": "run-p watch:* copy",
    "watch:client": "ng build --watch",
    "watch:server": "tsc --project src/server --watch",
    "nodemon": "nodemon --inspect --watch dist/server",
    "lint": "run-s -cs lint:*",
    "lint:client": "tslint -c src/client/tslint.json -p src/client/tsconfig.json",
    "lint:server": "tslint -p src/server/tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint"
  },
  "dependencies": {
    "@angular/animations": "^10.2.0",
    "@angular/common": "^10.2.0",
    "@angular/compiler": "^10.2.0",
    "@angular/core": "^10.2.0",
    "@angular/forms": "^10.2.0",
    "@angular/platform-browser": "^10.2.0",
    "@angular/platform-browser-dynamic": "^10.2.0",
    "@angular/router": "^10.2.0",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "angular-split": "^4.0.0",
    "cheerio": "^1.0.0-rc.3",
    "config": "^3.3.2",
    "core-js": "^3.6.5",
    "fluent-ffmpeg": "^2.1.2",
    "glob": "^7.1.6",
    "handlebars-helpers": "^0.10.0",
    "hbs": "^4.1.1",
    "i18n": "^0.13.2",
    "inversify": "^5.0.1",
    "inversify-express-utils": "^6.3.2",
    "lodash": "^4.17.20",
    "log4js": "^6.3.0",
    "mime-types": "^2.1.27",
    "moment": "^2.29.1",
    "named-js-regexp": "^1.3.5",
    "pouchdb": "^7.2.2",
    "pouchdb-find": "^7.2.2",
    "primeflex": "^2.0.0",
    "primeicons": "^4.0.0",
    "primeng": "^10.0.3",
    "puppeteer": "^10.2.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.3",
    "zone.js": "^0.11.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1002.0",
    "@angular/cdk": "^10.2.5",
    "@angular/cli": "^10.2.0",
    "@angular/compiler-cli": "^10.2.0",
    "@angular/language-service": "^10.2.0",
    "@types/cheerio": "^0.22.22",
    "@types/config": "0.0.36",
    "@types/express": "^4.17.8",
    "@types/fluent-ffmpeg": "^2.1.16",
    "@types/glob": "^7.1.3",
    "@types/handlebars-helpers": "^0.5.2",
    "@types/hbs": "^4.0.1",
    "@types/i18n": "^0.8.8",
    "@types/lodash": "^4.14.162",
    "@types/mime-types": "^2.1.0",
    "@types/moment": "^2.13.0",
    "@types/node": "^14.14.5",
    "@types/pouchdb": "^6.4.0",
    "@types/pouchdb-find": "^6.3.6",
    "codelyzer": "~6.0.1",
    "copyfiles": "^2.4.0",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "typescript": "~4.0.5"
  }
}
