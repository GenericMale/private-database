{
  "name": "private-database",
  "version": "0.0.1",
  "description": "Private Database",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GazonkFoo/private-database.git"
  },
  "license": "MIT",
  "main": "dist/server/main.js",
  "scripts": {
    "start": "node dist/server/main.js",
    "build": "run-s clean build:* copy",
    "build:client": "ng build --configuration production",
    "build:server": "tsc --project src/server",
    "clean": "rimraf dist",
    "copy": "copyfiles -u 2 \"src/server/templates/**/*\" dist/server/",
    "serve": "run-p watch nodemon",
    "watch": "run-p watch:* copy",
    "watch:client": "ng build --watch",
    "watch:server": "tsc --project src/server --watch",
    "nodemon": "nodemon --inspect --watch dist/server",
    "lint": "run-s -cs lint:*",
    "lint:client": "eslint src/client --ext .ts,.html",
    "lint:server": "eslint src/server --ext .ts",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint"
  },
  "dependencies": {
    "@angular/animations": "^12.2.6",
    "@angular/cdk": "^12.2.6",
    "@angular/common": "^12.2.6",
    "@angular/compiler": "^12.2.6",
    "@angular/core": "^12.2.6",
    "@angular/forms": "^12.2.6",
    "@angular/platform-browser": "^12.2.6",
    "@angular/platform-browser-dynamic": "^12.2.6",
    "@angular/router": "^12.2.6",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "angular-split": "^5.0.0",
    "cheerio": "^1.0.0-rc.10",
    "config": "^3.3.6",
    "core-js": "^3.18.0",
    "fluent-ffmpeg": "^2.1.2",
    "glob": "^7.1.7",
    "handlebars-helpers": "^0.10.0",
    "hbs": "^4.1.2",
    "i18n": "^0.13.3",
    "inversify": "^5.1.1",
    "inversify-express-utils": "^6.3.2",
    "lodash": "^4.17.21",
    "log4js": "^6.3.0",
    "mime-types": "^2.1.32",
    "moment": "^2.29.1",
    "named-js-regexp": "^1.3.5",
    "pouchdb": "^7.2.2",
    "pouchdb-find": "^7.2.2",
    "primeflex": "^3.0.1",
    "primeicons": "^4.1.0",
    "primeng": "^12.1.1",
    "puppeteer": "^10.2.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.7",
    "zone.js": "^0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.2.6",
    "@angular-eslint/builder": "12.5.0",
    "@angular-eslint/eslint-plugin": "12.5.0",
    "@angular-eslint/eslint-plugin-template": "12.5.0",
    "@angular-eslint/schematics": "12.5.0",
    "@angular-eslint/template-parser": "12.5.0",
    "@angular/cli": "^12.2.6",
    "@angular/compiler-cli": "^12.2.6",
    "@angular/language-service": "^12.2.6",
    "@types/cheerio": "^0.22.30",
    "@types/config": "0.0.36",
    "@types/express": "^4.17.13",
    "@types/fluent-ffmpeg": "^2.1.18",
    "@types/glob": "^7.1.4",
    "@types/handlebars-helpers": "^0.5.3",
    "@types/hbs": "^4.0.1",
    "@types/i18n": "^0.8.8",
    "@types/lodash": "^4.14.173",
    "@types/mime-types": "^2.1.1",
    "@types/moment": "^2.13.0",
    "@types/node": "^14.14.5",
    "@types/pouchdb": "^6.4.0",
    "@types/pouchdb-find": "^6.3.7",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "copyfiles": "^2.4.1",
    "eslint": "^7.26.0",
    "nodemon": "^2.0.12",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "~4.3.5"
  }
}
